      "단어입력 : apple\n",
      "찾을문자 : e\n",
      "5 번째에  e 가 있음\n"
     ]
    }
   ],
   "source": [
    "word = input(\"단어입력 : \")\n",
    "test = input(\"찾을문자 : \")\n",
    "\n",
    "count = 0\n",
    "\n",
    "for char in word :\n",
    "    count = count + 1\n",
    "    if char == test :\n",
    "        print(count, \"번째에 \", test, \"가 있음\")\n",
    "        break\n",
    "    elif count >= len(word) :\n",
    "        print(\"아무것도 없음\")\n",
    "        break"
   ]
  }
