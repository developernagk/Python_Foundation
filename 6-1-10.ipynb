    "import turtle\n",
    "\n",
    "wn = turtle.Screen()\n",
    "wn.bgcolor(\"lightblue\")\n",
    "wn.title(\"Draw Triangle\")\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.color(\"hotpink\")\n",
    "t.pensize(3)\n",
    "\n",
    "t.goto(0, 250)\n",
    "\n",
    "for i in range(3) :\n",
    "    t.forward(80)\n",
    "    t.left(120)\n",
    "    \n",
    "t.right(180)\n",
    "t.forward(80)"
   ]
  }
