    "import turtle\n",
    "\n",
    "win = turtle.Screen()\n",
    "win.bgcolor(\"black\")\n",
    "\n",
    "one = turtle.Turtle()\n",
    "one.color(\"red\")\n",
    "one.pensize(3)\n",
    "\n",
    "def n_one(n, size) :\n",
    "    for i in range(n) :\n",
    "        one.circle(size)\n",
    "        one.left(360.0 / n)\n",
    "        \n",
    "n_one(20, 70)"
   ]
  }
