    "\n",
    "star = turtle.Turtle()\n",
    "star.color(\"red\")\n",
    "\n",
    "for i in range(20) :\n",
    "    star.forward(i * 50)\n",
    "    star.right(144)\n",
    "    \n",
    "turtle.done()"
